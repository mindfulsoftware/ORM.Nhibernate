<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping 
  xmlns="urn:nhibernate-mapping-2.2"
  assembly="ORM.Nhibernate"
  namespace="ORM.Nhibernate.Model">

  <class name="Shipper" table="Shippers">
    <id name="ShipperId">
      <generator class="identity" />
    </id>

    <property name="CompanyName" />
    <property name="Phone" />
  </class>

</hibernate-mapping>
